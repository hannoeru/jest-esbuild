{
  "name": "jest-esbuild",
  "version": "0.3.0",
  "description": "A Jest esbuild transformer.",
  "packageManager": "pnpm@7.33.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hannoeru/jest-esbuild"
  },
  "author": "hannoeru <me@hanlee.co>",
  "files": [
    "dist",
    "jest-preset.js",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/hannoeru/jest-esbuild",
  "bugs": "https://github.com/hannoeru/jest-esbuild/issues",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "example:test": "npm -C example run test"
  },
  "peerDependencies": {
    "jest": ">=27"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "esbuild": "^0.20.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@hannoeru/eslint-config": "^0.8.10",
    "@jest/transform": "^28.1.2",
    "@types/debug": "^4.1.12",
    "@types/jest": "^28.1.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
